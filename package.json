    "zip-a-folder": "^1.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "cspell": "^4.1.0",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "prettier": "^2.1.1",
    "typescript": "^4.0.2"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
