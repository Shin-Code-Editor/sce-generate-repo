    "sce-gen": "./build/main/index.js"
  },
  "typings": "build/main/index.d.ts",
  "repository": "https://github.com/shin-code-editor/sce-generate-repo",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build": "yarn build:main",
